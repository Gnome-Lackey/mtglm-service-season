{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "SeasonPlayerMetadataResponseSchema",
  "required": ["season", "player", "seasonWins", "seasonLosses", "totalWins", "totalLosses"],
  "properties": {
    "season": {
      "type": "string",
      "title": "SeasonId"
    },
    "player": {
      "type": "string",
      "title": "PlayerId"
    },
    "seasonWins": {
      "type": "integer",
      "title": "SeasonMetadataSeasonWins"
    },
    "seasonLosses": {
      "type": "integer",
      "title": "SeasonMetadataSeasonLosses"
    },
    "totalWins": {
      "type": "integer",
      "title": "SeasonMetadataTotalWins"
    },
    "totalLosses": {
      "type": "integer",
      "title": "SeasonMetadataTotalLosses"
    },
    "matches": {
      "type": "array",
      "title": "SeasonMetadataMatchIds",
      "items": {
        "type": "string"
      }
    },
    "playedOpponents": {
      "type": "array",
      "title": "SeasonMetadataPlayerIds",
      "items": {
        "type": "string"
      }
    }
  }
}

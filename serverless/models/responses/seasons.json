{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "title": "SeasonsResponseSchema",
  "items": {
    "type": "object",
    "title": "Season",
    "properties": {
      "id": {
        "type": "string",
        "title": "SeasonId"
      },
      "players": {
        "type": "array",
        "items": {
          "type": "string",
          "title": "SeasonPlayerIds"
        }
      },
      "set": {
        "type": "object",
        "title": "SeasonSet",
        "properties": {
          "id": {
            "type": "string",
            "title": "SeasonSetId"
          },
          "code": {
            "type": "string",
            "title": "SeasonSetCode"
          },
          "name": {
            "type": "string",
            "title": "SeasonSetName"
          },
          "releasedOn": {
            "type": "string",
            "title": "SeasonSetReleasedOn"
          },
          "cardCount": {
            "type": "string",
            "title": "SeasonSetCardCount"
          }
        }
      },
      "isActive": {
        "type": "boolean",
        "title": "SeasonIsActive"
      },
      "startedOn": {
        "type": "string",
        "title": "SeasonDateStarted"
      },
      "endedOOn": {
        "type": "string",
        "title": "SeasonDateEnded"
      },
      "top": {
        "type": "array",
        "items": {
          "type": "string",
          "title": "SeasonTopPlayerIds"
        }
      }
    }
  }
}

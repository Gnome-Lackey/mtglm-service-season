    "title": "Season",
    "properties": {
      "id": {
        "type": "string",
        "title": "SeasonId"
      },
      "players": {
        "type": "array",
        "items": {
          "type": "string",
          "title": "SeasonPlayerIds"
        }
      },
      "matches": {
        "type": "array",
        "items": {
          "type": "string",
          "title": "SeasonMatchIds"
        }
      },
      "isActive": {
        "type": "boolean",
        "title": "SeasonIsActive"
      },
      "set": {
        "type": "string",
        "title": "SeasonSetId"
      },
      "startedOn": {
        "type": "string",
        "title": "SeasonDateStarted"
      },
      "endedOOn": {
        "type": "string",
        "title": "SeasonDateEnded"
      },
      "top": {
        "type": "array",
        "items": {
          "type": "string",
          "title": "SeasonTopPlayerIds"
        }
      }
    }
